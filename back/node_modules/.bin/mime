#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pau/Telefonica/trustos/login-cert/back/node_modules/mime/node_modules:/home/pau/Telefonica/trustos/login-cert/back/node_modules:/home/pau/Telefonica/trustos/login-cert/node_modules:/home/pau/Telefonica/trustos/node_modules:/home/pau/Telefonica/node_modules:/home/pau/node_modules:/home/node_modules:/node_modules:/home/pau/Telefonica/trustos/login-cert/back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pau/Telefonica/trustos/login-cert/back/node_modules/mime/node_modules:/home/pau/Telefonica/trustos/login-cert/back/node_modules:/home/pau/Telefonica/trustos/login-cert/node_modules:/home/pau/Telefonica/trustos/node_modules:/home/pau/Telefonica/node_modules:/home/pau/node_modules:/home/node_modules:/node_modules:/home/pau/Telefonica/trustos/login-cert/back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
